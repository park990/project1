<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test_anw">
    <select id="getc_anw" parameterType="pm.vo.testDTO" resultType="pm.vo.test_anw_tVO">
        SELECT test_anw_t_idx, test_idx, ques_num, c_anw
        FROM test_anw_t
        WHERE test_idx = #{test_idx}
    </select>
</mapper>