<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--수강중인 학생-->
<mapper namespace="lec_std_t"> <!--테이블 명 or 파일명-->

    <select id="enrolledStudent" resultType="pm.vo.EnrolledStudentVO" parameterType="Map">
        SELECT es.std_name, es.lec_name, s.std_phone
        FROM lec_std_t es INNER JOIN std_t s
        ON es.stdno = s.stdno
        WHERE lec_no = #{lec_no}
    </select>


</mapper>